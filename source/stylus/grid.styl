.row {
	position: fixed
	width: 100%
	height: 100%
  max-width: maximum-width
	pointer-events: none
  opacity: 1
  z-index: 200

	for i in (0..20) {
		.column-{i} {
  		width: 1px
  		margin-left: lookup('percentage-' + i)
  		height: 100%
  		position: absolute
  		float: left
  		background-color: grid-color
  		opacity: 0
  	}
	}
}
