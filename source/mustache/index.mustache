<!DOCTYPE html>
<html>
  <head>
    {{> head }}
    <script type="text/javascript" src="js/pagination.js"></script>
    <title>Ramon Gilabert</title>
  </head>
  <body id='myself'>
    <h1 id='page-title'>Ramon Gilabert</h1>
    {{> header }}
    <div id='sections'>
      {{# myself }}
      <section id="{{ kind }}">
        <figure></figure>
        <div class='wrapper'>
          <h2>{{ title }}</h2>
          <p><strong>{{ span }}</strong>{{{ subtitle }}}</p>
          <a href='./{{ link }}'>read more</a>
        </div>
      </section>
      {{/ myself }}
    </div>
    <nav>
      {{# myself }}
      <div><button class='navigator' id='navigation-{{ kind }}'>{{ kind }}</button></div>
      {{/ myself }}
      <div class='disclosure disclosure-projects'><p>projects</p></div>
    </nav>
    <figure id='triangle'></figure>
    <div id='indicator'></div>
    <div class='corner-glitch bottom-corner-glitch'></div>
    <button id='back-top-button'>back top</button>
    <div id='selected-work'><div id='selected-wrapper'><p>selected</p></div></div>
  </body>
</html>
